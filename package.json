    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "nats": "^2.28.2",
    "node-fetch": "^3.3.2",
    "openapi-fetch": "^0.9.3",
    "openapi-typescript": "^6.7.5"    
  },
  "packageManager": "yarn@4.0.1"
}
