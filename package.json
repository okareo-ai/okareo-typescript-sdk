{
  "name": "okareo-ts-sdk",
  "version": "0.0.52",
  "main": "dist/index.min.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Okareo @2024",
    "email": "support@okareo.com"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/okareo-ai/okareo-typescript-sdk",
    "type": "git"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "gen-api": "npx openapi-typescript https://api.okareo.com/openapi.json -o ./src/api/v1/okareo_endpoints.ts",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "release": "yarn build && npm publish --provenance --access public",
    "test": "yarn build && jest",
    "test:ci": "cross-env CI=1 jest",
    "test:ts": "tsc --noEmit",
    "lint": "eslint",
    "lint-fix": "eslint --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lru-cache": "^10.2.1",
    "postcss": "^8.4.21",
    "prettier": "^3.2.5",
    "rollup": "^2.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
